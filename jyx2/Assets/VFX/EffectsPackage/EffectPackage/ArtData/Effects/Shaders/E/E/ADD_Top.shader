			float2 uv_MainTex = i.uv_texcoord * _MainTex_ST.xy + _MainTex_ST.zw;
			float4 tex2DNode5 = tex2D( _MainTex, uv_MainTex );
			c.rgb = ( ( i.vertexColor * tex2DNode5 * _MainColor ) * ( i.vertexColor.a * tex2DNode5.a * _MainColor.a ) ).rgb;
			c.a = 1;
